.Work {
  padding: 10px;
  border: 1px solid #e7e8e9;
  background-color: #fff;
  margin-bottom: 10px;
  border-radius: 5px;
  img {
    width auto;
    max-width: 100%;
    max-height: 70px;
  }
  h2 {
    font-size: 1rem;
    margin-bottom 0;
  }
  .title {
    margin-left 10px;
    max-width 260px;
    display inline-block;
    vertical-align top;
  }
}

.frontpage {
  width 80px;
  display: inline-block;
}

.searchresult {
  background-color #d9d8e0;
  padding 20px 10px
  h1 {
    font-weight: 300;
    text-align center;
    margin 20px 0;
  }
}

.close-button {
  position: absolute;
  right: 5px;
  top: 0;
  img {
    width: 45px;
  }
}